<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AnahiQuezadaBurgerMauiApp.MainPage">

    <StackLayout>
        <Label Text="Burger App Anahi Quezada"
        HorizontalOptions="Center"
        VerticalOptions="Center"></Label>
        <Button Text="Hamburguesas"
            Clicked="Button_Clicked_JDS"
            HorizontalOptions="Center"></Button>
        <ListView x:Name="AQlistView">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextCell x:DataType="{Binding AQname}"></TextCell>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </StackLayout>
</ContentPage>
